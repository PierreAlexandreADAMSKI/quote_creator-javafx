<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import app.view.main.widgets.table_box.TableViewBox?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.view.main.MainController">
    <children>
        <FlowPane prefHeight="800.0" prefWidth="1280.0">
            <children>
                <ToolBar prefHeight="40.0" prefWidth="1280.0">
                    <items>
                        <MenuButton fx:id="newMenu" mnemonicParsing="false" text="nouveau"/>
                        <Button layoutX="10.0" layoutY="11.0" mnemonicParsing="false" text="Load"/>
                        <Button layoutX="59.0" layoutY="11.0" mnemonicParsing="false" text="save"/>
                        <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButtonAction" text="Add"
                                translateX="990.0"/>
                        <Button fx:id="delButton" layoutX="179.0" layoutY="11.0" mnemonicParsing="false" text="Del"
                                translateX="990.0" onAction="#onDelButtonAction"/>
                    </items>
                </ToolBar>
                <VBox prefHeight="720.0" prefWidth="400.0">
                    <children>
                        <HBox>
                            <children>
                                <ScrollPane fx:id="formScrollPane" prefHeight="300.0" prefWidth="400.0"
                                            fitToWidth="true"
                                            hbarPolicy="NEVER"
                                            vbarPolicy="ALWAYS">
                                </ScrollPane>
                            </children>
                        </HBox>
                        <HBox layoutY="300.0">
                            <children>
                                <TreeView fx:id="treeView" layoutY="300.0" prefHeight="420.0" prefWidth="400.0"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <ScrollPane fx:id="tableScrollPane" hbarPolicy="AS_NEEDED" prefHeight="600.0" prefWidth="880.0"
                            vbarPolicy="ALWAYS"/>
            </children>
        </FlowPane>
    </children>
</AnchorPane>
