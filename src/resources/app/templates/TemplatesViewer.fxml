<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<fx:root fx:id="rootPane" type="app.templates.TemplatesViewer" maxHeight="-Infinity"
         maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
         xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane gridLinesVisible="true" prefHeight="800.0" prefWidth="1280.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="380.0" minWidth="10.0" prefWidth="380.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="900.0" minWidth="10.0" prefWidth="900.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TabPane prefHeight="800.0" prefWidth="270.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="Untitled Tab 1"/>
                        <Tab text="Untitled Tab 2"/>
                    </tabs>
                </TabPane>
                <ScrollPane prefHeight="800.0" prefWidth="530.0" GridPane.columnIndex="1">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="800.0" prefWidth="900.0">
                            <children>
                                <HBox fx:id="imageViewBox" alignment="CENTER" prefHeight="800.0" prefWidth="900.0">
                                    <children>
                                        <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0"
                                                   pickOnBounds="true" preserveRatio="true"/>
                                    </children>
                                </HBox>
                            </children>
                        </AnchorPane>
                    </content>
                </ScrollPane>
            </children>
        </GridPane>
    </children>
</fx:root>
